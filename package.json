{
  "name": "portal-js",
  "title": "portal-js",
  "description": "portal-js",
  "version": "0.1.0",
  "author": "Evan Caldwell",
  "homepage": "https://github.com/ecaldwell/portal-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ecaldwell/portal-js.git"
  },
  "keywords": [
    "ArcGIS Online",
    "Portal for ArcGIS",
    "javascript",
    "REST"
  ],
  "main": "./build/portal.js",
  "jsnext:main": "./src/index",
  "engines": {
    "node": ">= 0.12.7"
  },
  "devDependencies": {
    "documentation": "^3.0.3",
    "request": "^2.65.0",
    "rollup": "^0.20.5",
    "rollup-plugin-babel": "^2.0.0",
    "rollup-plugin-commonjs": "^1.3.0",
    "rollup-plugin-npm": "^1.0.0"
  },
  "dependencies": {
    "request": "^2.65.0"
  },
  "scripts": {
    "document": "documentation --format md --output documentation.md",
    "rollup:es6": "rollup --format es6 src/index.js --output build/portal.es6.js",
    "rollup:cjs": "rollup --format cjs src/index.js --output build/portal.cjs.js",
    "rollup:amd": "rollup --format amd src/index.js --output build/portal.amd.js",
    "rollup:umd": "rollup --format umd src/index.js --output build/portal.umd.js",
    "rollup:config": "rollup --config rollup.config.js"
  }
}
